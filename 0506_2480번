#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int a, b, c, sum;


int main() {
	scanf("%d %d %d", &a, &b, &c);

	if (a == b && b == c)
		sum = 10000 + a * 1000;
	else if (a == b || a == c || b == c) {
		if (a == b || a == c)
			sum = 1000 + a * 100;
		else
			sum = 1000 + b * 100;
	}
	else {
		if (a > b && a > c)
			sum = a * 100;
		if (b > a && b > c)
			sum = b * 100;
		if (c > a && c > b)
			sum = c * 100;
	}

	printf("%d", sum);

	return 0;
}
