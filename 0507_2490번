#include <stdio.h>


int main() {
	int a,num = 0;

	for (int i = 0; i < 3; i++) {
		num = 0;
		for (int j = 0; j < 4; j++) {
			scanf("%d", &a);
			if (a == 0) {
				num++;
			}
		}
		if (num == 1)
			printf("A\n");
		else if (num == 2)
			printf("B\n");
		else if (num == 3)
			printf("C\n");
		else if (num == 4)
			printf("D\n");
		else
			printf("E\n");
		

	}

	return 0;
}
